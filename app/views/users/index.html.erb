<h1>All users are listed here</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.email%>
      <p><%= button_to "Edit user", edit_user_path(user), method: :get, class: "btn btn-primary" %></>    
      <%= button_to "Delete", user_path(user), method: :delete%>
    </li>
  <% end %>
</ul>
